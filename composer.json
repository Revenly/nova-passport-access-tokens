{
    "name": "revenly/nova-passport-access-tokens",
    "description": "Create passport access tokens for users",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "lacasera",
            "email": "aboateng62@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "R64\\NovaPassportAccessTokens\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "R64\\NovaPassportAccessTokens\\Providers\\NovaPassportAccessTokenServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "laravel/nova": "^3.9",
        "laravel/passport": "^10.0|^11.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "rector/rector": "^0.15.16"
    }
}
